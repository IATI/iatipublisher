        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-vue": "^8.4.1",
        "husky": "^7.0.4",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "prettier-eslint": "^13.0.0",
        "prettier-eslint-cli": "^5.0.1"
    }
